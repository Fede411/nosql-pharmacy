                                        CA01</label>
                                    <label class="gfh-checkbox"><input type="checkbox" name="gfh_ids" value="ALEDP">
                                        ALEDP</label>
                                    <label class="gfh-checkbox"><input type="checkbox" name="gfh_ids" value="ADOM">
                                        ADOM</label>
                                    <label class="gfh-checkbox"><input type="checkbox" name="gfh_ids" value="ACVDP">
                                        ACVDP</label>
                                </div>

                                <p></p>
                            </fieldset>
                            <button type="submit" class="btn btn-primary filter-button"
                                onclick="countPacienteNuevoByDate()">Count</button>
                        </form>
                    </div>







                </div>
            </div>


            <div class="col-xl-9 col-lg-8 col-md-9 col-sm-12">
                <div class="row content-info">
                    <div class="page-header">
                        <h1 id="tables">Statistics</h1>
                    </div>


                    <% if (typeof start !=='undefined' && typeof end !=='undefined' ) { %>
                        <div class="date-range-info">
                            <strong>Analysis period:</strong>
                            <%= new Date(start).toLocaleDateString('es-ES') %> - <%= new
                                    Date(end).toLocaleDateString('es-ES') %>
                        </div>
                        <% } %>

                            <!-- Main statistics card -->
                            <div class="col-12">
                                <div class="stats-card text-center">
                                    <div class="stats-label">TOTAL PATIENTS</div>
                                    <div class="stats-number">
                                        <%= cuenta %>
                                    </div>
                                    <div class="text-muted">
                                        <% if (typeof gfh_stats !=='undefined' && Object.keys(gfh_stats).length> 0) { %>
                                            Distributed across <%= Object.keys(gfh_stats).length %> services (GFH)
                                                <% } %>
                                    </div>
                                </div>
                            </div>

                            <!-- Charts -->
                            <div class="col-md-6">
                                <div class="stats-card">
                                    <h4>Distribution by GFH</h4>
                                    <div class="chart-container">
                                        <canvas id="gfhBarChart"></canvas>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6">
                                <div class="stats-card">
                                    <h4>Proportion by GFH</h4>
                                    <div class="chart-container">
                                        <canvas id="gfhPieChart"></canvas>
                                    </div>
                                </div>
                            </div>

                            <!-- Detailed table by GFH -->
                            <div class="col-12">
                                <div class="stats-card">


                                    <div class="gfh-table">
                                        <table class="table table-hover table-striped">
                                            <thead class="bg-secondary">
                                                <tr>
                                                    <th scope="col">GFH</th>
                                                    <th scope="col">Number of Patients</th>
                                                    <th scope="col">Percentage</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% if (typeof gfh_stats !=='undefined' && Object.keys(gfh_stats).length>
                                                    0) { %>
                                                    <% // Sort by number of patients descending const
                                                        sortedGFH=Object.entries(gfh_stats).sort((a, b)=> b[1] - a[1]);
                                                        sortedGFH.forEach(function([gfh, count]) {
                                                        const percentage = ((count / cuenta) * 100).toFixed(2);
                                                        %>
                                                        <tr>
                                                            <td><strong>
                                                                    <%= gfh %>
                                                                </strong></td>
                                                            <td>
                                                                <%= count %>
                                                            </td>
                                                            <td>
                                                                <div class="progress">
                                                                    <div class="progress-bar" role="progressbar"
                                                                        aria-valuenow="<%= percentage %>"
                                                                        aria-valuemin="0" aria-valuemax="100">
                                                                        <%= percentage %>%
                                                                    </div>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <% }); %>
                                                            <% } else { %>
                                                                <tr>
                                                                    <td colspan="3" class="text-center">No GFH data
                                                                        available</td>
                                                                </tr>
                                                                <% } %>
                                            </tbody>
                                            <tfoot class="bg-light">
                                                <tr>
                                                    <th>TOTAL</th>
                                                    <th>
                                                        <%= cuenta %>
                                                    </th>
                                                    <th>100%</th>
                                                </tr>
                                            </tfoot>
                                        </table>
                                    </div>
                                </div>
                            </div>

                            <!-- Optional collapsible table with all patients -->
                            <div class="col-12">
                                <div class="stats-card">
                                    <button class="btn btn-info" type="button" data-toggle="collapse"
                                        data-target="#patientList">
                                        <i class="fas fa-list"></i> View complete patient list
                                    </button>

                                    <div class="collapse" id="patientList">
                                        <div class="mt-3" style="max-height: 500px; overflow-y: auto;">
                                            <table class="table table-sm table-hover">
                                                <thead class="thead-light">
                                                    <tr>
                                                        <th>Patient ID</th>
                                                        <th>Date</th>
                                                        <th>GFH</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% if (datos && datos.length> 0) { %>
                                                        <% datos.forEach(function(dispensacion) { %>
                                                            <tr>
                                                                <td>
                                                                    <%= dispensacion.paciente_id || '' %>
                                                                </td>
                                                                <td>
                                                                    <%= dispensacion.fecha_inicio ? new
                                                                        Date(dispensacion.fecha_inicio).toLocaleDateString('es-ES')
                                                                        : 'No date' %>
                                                                </td>
                                                                <td>
                                                                    <span class="badge badge-secondary">
                                                                        <%= dispensacion.gfh_id || 'N/A' %>
                                                                    </span>
                                                                </td>
                                                            </tr>
                                                            <% }); %>
                                                                <% } %>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>

                </div>
            </div>
        </div>


        <script>
            $(document).ready(function () {
                $('.pacient-alert').fadeOut({ duration: 3000 });

                if ($('div.input-daterange').length) {
                    $('div.input-daterange').datepicker({
                        format: 'yyyy-mm-dd'
                    });
                }

                if (gfh_stats && Object.keys(gfh_stats).length > 0) {
                    const gfhData = JSON.stringify(gfh_stats);
                    const sortedEntries = Object.entries(gfhData).sort((a, b) => b[1] - a[1]);
                    const labels = sortedEntries.map(entry => entry[0]);
                    const values = sortedEntries.map(entry => entry[1]);
                    const colors = generateColors(labels.length);

                    const barCanvas = document.getElementById('gfhBarChart');
                    if (barCanvas) {
                        new Chart(barCanvas.getContext('2d'), {
                            type: 'bar',
                            data: {
                                labels: labels,
                                datasets: [{
                                    label: 'Nmero de Pacientes',
                                    data: values,
                                    backgroundColor: 'rgba(54,162,235,0.6)',
                                    borderColor: 'rgba(54,162,235,1)',
                                    borderWidth: 1
                                }]
                            },
                            options: { responsive: true, maintainAspectRatio: false }
                        });
                    }

                    const pieCanvas = document.getElementById('gfhPieChart');
                    if (pieCanvas) {
                        new Chart(pieCanvas.getContext('2d'), {
                            type: 'pie',
                            data: {
                                labels: labels,
                                datasets: [{
                                    data: values,
                                    backgroundColor: colors,
                                    borderColor: '#fff',
                                    borderWidth: 2
                                }]
                            },
                            options: { responsive: true, maintainAspectRatio: false }
                        });
                    }
                }
            });

            function generateColors(count) {
                const colors = [];
                for (let i = 0; i < count; i++) {
                    const hue = (i * 360 / count) % 360;
                    colors.push(`hsla(${hue},70%,60%,0.8)`);
                }
                return colors;
            }

            function exportToCSV() {
                if (gfh_stats) {
                    const data = JSON.stringify(gfh_stats);
                    let csv = 'GFH,Nmero de Pacientes,Porcentaje\n';
                    const total = cuenta;

                    Object.entries(data).forEach(([gfh, count]) => {
                        const percentage = ((count / total) * 100).toFixed(2);
                        csv += `${gfh},${count},${percentage}%\n`;
                    });

                    const blob = new Blob([csv], { type: 'text/csv' });
                    const url = URL.createObjectURL(blob);
                    const a = document.createElement('a');
                    a.href = url;
                    a.download = 'estadisticas_pacientes_gfh.csv';
                    a.click();
                }
            }
        </script>


</body>

</html>